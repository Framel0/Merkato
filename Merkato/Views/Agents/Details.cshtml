            <div id="otherInfoTab" class="tab-pane fade">
                <div class="box box-info">
                    <div class="box-body">
                        <hr />
                        <div class="form-horizontal">
                            <div class="col-md-12">
                                <div class="col-md-6">
                                    <div class="col-md-11">
                                        <div class="form-group">
